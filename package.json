{
  "name": "tailvue",
  "version": "1.0.0",
  "description": "windicss, tailwindcss and unocss components for Vue",
  "repository": "https://github.com/fumeapp/tailvue",
  "author": "Kevin Olson",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vue3",
    "vue-3",
    "vue",
    "vue-plugin",
    "windicss",
    "tailwindcss"
  ],
  "files": [
    "dist",
    "src/types/*",
    "src/typings.d.ts",
    "web-types"
  ],
  "types": "src/typings.d.ts",
  "main": "./dist/tailvue.umd.js",
  "module": "./dist/tailvue.es.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/tailvue.es.js",
        "types": "./src/typings.d.ts"
      },
      "require": {
        "default": "./dist/tailvue.umd.js",
        "types": "./src/typings.d.ts"
      }
    }
  },
  "scripts": {
    "dev": "vite playground",
    "build": "vite build",
    "postbuild": "echo '// @unocss-include' >> dist/tailvue.es.js; echo '// @unocss-include' >> dist/tailvue.umd.js"
  },
  "dependencies": {
    "vue": "^3.2.21"
  },
  "peerDependencies": {
    "vue": "^3.2.21"
  },
  "devDependencies": {
    "@iconify/vue": "^3.1.0",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "@types/node": "^20.2.5",
    "@unocss/transformer-directives": "^0.52.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.11",
    "@vueuse/core": "^10.1.2",
    "esbuild": "0.14.32",
    "simple-syntax-highlighter": "^2.2.0",
    "tsup": "^5.7.0",
    "unbuild": "^0.6.9",
    "unocss": "^0.52.4",
    "vite": "~4.3.9",
    "vite-plugin-windicss": "^1.5.1",
    "windicss": "^3.2.1"
  },
  "web-types": "web-types/web-types.json"
}
